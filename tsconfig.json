{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2022",
		"outDir": "out",
		"lib": ["ES2022", "DOM"],
		"sourceMap": true,
		"rootDir": ".",
		"strict": true,
		"jsx": "react",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"exclude": ["node_modules", ".vscode-test", "app", "preview"]
}